{
  "name": "course-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.6.0",
    "@storybook/test-runner": "0.0.2-canary.e62f9ee.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.1.9",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.3",
    "@types/react-dom": "^17.0.8",
    "axios": "^0.21.1",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hooks-helper": "^1.6.0",
    "react-loading-skeleton": "^2.2.0",
    "react-lottie-player": "^1.3.1",
    "react-multi-carousel": "^2.6.3",
    "react-redux": "^7.2.4",
    "react-router-dom": "^6.0.0",
    "react-scripts": "^4.0.3",
    "react-transition-group": "^4.4.2",
    "styled-components": "^5.3.0",
    "typescript": "^4.3.4",
    "use-dark-mode": "^2.3.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "SKIP_PREFLIGHT_CHECK=true react-scripts build",
    "test": "react-scripts test",
    "test:ci": "yarn test --watchAll=false",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public -o build/storybook",
    "build-all": "yarn build && yarn build-storybook",
    "chromatic": "chromatic --project-token $CHROMATIC_PROJECT_TOKEN --exit-zero-on-changes --only-changed",
    "test-storybook:jsdom": "jest --config ./jsdom-jest.config.js",
    "test-storybook": "test-storybook",
    "test-storybook-ci:playwright": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue,green\" \"yarn build-storybook && npx http-server build/storybook --port 6006 --silent\" \"wait-on tcp:6006 && yarn test-storybook\"",
    "prepare": "husky install",
    "lint": "eslint --fix .",
    "prettier": "prettier --write .",
    "pre-commit": "lint-staged"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@storybook/addon-a11y": "^6.5.0-alpha.3",
    "@storybook/addon-essentials": "^6.5.0-alpha.3",
    "@storybook/addon-interactions": "^6.5.0-alpha.3",
    "@storybook/addon-storyshots": "^6.5.0-alpha.3",
    "@storybook/jest": "^0.0.5",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.5.0-alpha.3",
    "@storybook/testing-library": "^0.0.7",
    "@storybook/testing-react": "^1.2.2",
    "@types/jest-axe": "^3.5.1",
    "@types/react-hooks-helper": "^1.6.3",
    "@types/react-redux": "^7.1.16",
    "@types/react-transition-group": "^4.4.1",
    "@types/styled-components": "^5.1.10",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "babel-loader": "8.1.0",
    "chromatic": "^6.0.4",
    "concurrently": "^6.5.1",
    "cross-spawn": "^7.0.3",
    "eslint": "^7.11.0",
    "eslint-config-airbnb-typescript": "^16.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-storybook": "^0.5.3",
    "husky": ">=6",
    "jest": "^26.6.3",
    "jest-axe": "^4.1.0",
    "jest-canvas-mock": "^2.3.1",
    "lint-staged": ">=10",
    "msw": "^0.29.0",
    "msw-storybook-addon": "^1.5.0",
    "prettier": "^2.3.2",
    "react-is": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "storybook-addon-designs": "^6.2.0",
    "storybook-mobile": "^1.0.0",
    "wait-on": "^6.0.0"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --fix",
    "*.{html,md}": "prettier --write"
  },
  "resolutions": {
    "jest": "^26.6.3",
    "react-scripts/jest": "^26.6.3"
  }
}
